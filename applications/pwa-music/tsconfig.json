{
  "compilerOptions": {
    "module": "ES2020",
    "lib": [
      "ES2020",
      "DOM",
      "DOM.Iterable",
      "WebWorker"
    ],
    "moduleResolution": "node",
    "skipLibCheck": true,
    "downlevelIteration": true,
    "sourceMap": true,
    "experimentalDecorators": true,
    "noImplicitThis": true,
    "allowSyntheticDefaultImports": true,
    "target": "es6",
    "rootDirs": [
      "src/",
      "plugins/"
    ],
    "baseUrl": "./",
    "paths": {
      "@/static/*": [
        "static/*"
      ],
      "@/*": [
        "src/*"
      ],
    },
    "outDir": "dist/",
    "jsx": "react-jsx"
  }
}